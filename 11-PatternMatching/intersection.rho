new log(`rho:io:stdout`), register in {

  for (@{{@"name"!(_) | _} /\ {@"age"!(_) | _}} <= register){
    log!("Both name and age were in the data")
  }
  |

  register!(@"name"!(0))
  |
  register!(@"age"!(0))
  |
  register!(@"name"!(0) | @"age"!(0))
  |
  register!(@"name"!(0) | @"age"!(0) | @"height"!(175))
}
