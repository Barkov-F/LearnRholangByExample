new stdout(`rho:io:stdout`) in {
  // Use the rnode repl to test your code
  // Send messages like
  // @"P1"!("Send any message")
  // @"P2"!("Hope I win")

  // When Player one wins
  for (m2 <- @"P2"){
    for (m1 <- @"P1"){
      stdout!("Player one wins!")
    }
  }
  |
  // When player two wins
  for (m1 <- @"P1"){
    for (m2 <- @"P2"){
      stdout!("Player two wins!")
    }
  }
}
